#include <iostream>       // завдання 8
#include <string>

int main()
{
  int a=1;
  float b=12.21;
  float res;
  res=(0.81*pow(a,(1/3))-(1/2.125)*pow(b,(1/3))*pow(M_E,a));
  std::cout << res;
}









#include <iostream>      //завдання 14
#include <string>

int main()
{
  int a=1;
  float b=12.21;
  float res;
  res=(4.3*sin((a/b+1*M_PI)))/((b/a)*1-cos((a/b-1)*M_PI)+log(b));
  std::cout << res;
}
